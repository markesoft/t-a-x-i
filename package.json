    "@nuxt/image": "^1.11.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/device": "^3.2.4",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^10.1.2",
    "@nuxtjs/leaflet": "^1.3.2",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.2",
    "@vueuse/nuxt": "^13.9.0",
    "nuxt": "^4.2.0",
