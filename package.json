    "deepRebuild": "npm run clean && npm run prisma_generate && npm run build",
    "analyze": "nuxt build --analyze",
    "audit": "node ./scripts/performance-audit.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@dargmuesli/nuxt-cookie-control": "^9.1.6",
