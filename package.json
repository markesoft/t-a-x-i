{
  "name": "T-A-X-I",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "nuxt start",
    "build": "nuxt build && node ./scripts/fix-dirname.js",
    "build:optimized": "node ./scripts/optimized-build.js",
    "dev": "nuxt dev -o",
    "dev-host": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prisma_generate": "npx prisma generate",
    "prisma_generate_sql": "npx prisma generate --sql",
